{
  "name": "harookit-atom",
  "main": "./lib/harookit-atom",
  "version": "0.0.1",
  "description": "Haroo cloud client app for Atom editor",
  "keywords": [
    "haroocloud",
    "paas"
  ],
  "activationCommands": {
    "atom-workspace": [
      "harookit:list-toggle",
      "harookit:sign-in",
      "harookit:sign-out"
    ]
  },
  "repository": "https://github.com/soomtong/harookit-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.1"
  },
  "configSchema": {
    "showOnLeftSide": {
      "type": "boolean",
      "default": false,
      "description": "Show the harookit view on the left side of the editor instead of the right."
    },
    "harooCloudHost": {
      "title": "Haroo cloud host location (for your own service)",
      "type": "string",
      "default": "https://haroocloud.com"
    },
    "harooCloudUserId": {
      "title": "User ID",
      "type": "string",
      "default": "User ID",
      "description": "ID for Haroo cloud service"
    },
    "harooCloudUserPassword": {
      "title": "Password",
      "type": "string",
      "default": "User Password",
      "description": "Password for Haroo cloud user ID"
    }
  }
}
